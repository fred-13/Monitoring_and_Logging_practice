global:
  scrape_interval: 15s

alerting:
  alertmanagers:
  - static_configs:
    - targets: ['127.0.0.1:9093']

rule_files:
  - /etc/prometheus/all.rules

scrape_configs:

  - job_name: 'prometheus'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.10.6:9090']

  - job_name: 'nginx_exporter'
    scrape_interval: 5s
    static_configs:
      - targets: ['192.168.10.7:9113']
